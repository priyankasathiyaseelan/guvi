n1=int(input("Enter no 1: "))
n2=int(input("Enter no 2: "))
add=n1+n2
sub=n1-n2
mul=n1*n2
div=n1/n2
print("sum of value" ,n1, "and" ,n2,"is",add)
print("dfference of value" ,n1, "and" ,n2,"is",sub)
print("multiplication of value" ,n1, "and" ,n2,"is",mul)
print("division of value" ,n1, "and" ,n2,"is",div)
