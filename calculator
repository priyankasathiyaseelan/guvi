n1=int(input("Enter value 1: "))
n2=int(input("Enter value 2: "))
print("Add-1 , Sub-2 , Mul-3 , Div-4 , Mod-5 , Sqr-6")
n3=int(input("Enter your choice :"))
if(n3==1):
  print("Answer : ",n1+n2)
elif(n3==2):
  print("Answer : ",n1-n2)
elif(n3==3):
  print("Answer : ",n1*n2)  
elif(n3==4):
  print("Answer : ",n1/n2)
elif(n3==5):
  print("Answer : ",n1%n2)  
elif(n3==6):
  print("Answer : ",n1**n1)
